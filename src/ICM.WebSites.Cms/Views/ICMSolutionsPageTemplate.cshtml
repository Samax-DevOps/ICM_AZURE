    <hr class="divider">
    <!-- INTEGRATIONS-1
    ============================================= -->

    <section id="Services" class="py-100 features-section division">
        <div class="container">
            <!-- SECTION TITLE -->
            <div class="row justify-content-center">
                <div class="col-md-10 col-lg-9">
                    <div class="section-title mb-70">

                        <!-- Title -->
                        <h2 class="s-50 w-700">Services</h2>
                    </div>
                </div>
            </div>
            <!-- FEATURES-2 WRAPPER -->
            <div class="fbox-wrapper text-center">
                <div class="row row-cols-1 row-cols-md-4" style="margin-bottom: 20px;">
                    <!-- FEATURE BOX #1 -->
                    <div class="col" id="Sales">
                        <div class="fbox-2 fb-1 wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">

                            <!-- Image -->
                            <div class="fbox-img gr--whitesmoke h-175">
                                <img class="img-fluid" src="assets/icmsolutions/images/icon-1.png" alt="feature-image">
                            </div>

                            <!-- Text -->
                            <div class="fbox-txt">
                                <h6 class="s-22 w-700">Sales</h6>
                                <p>Accelerating Growth & Efficiency | Flexible, tailored solutions that fit your requirements.</p>
                            </div>

                        </div>
                    </div> <!-- END FEATURE BOX #1 -->
                    <!-- FEATURE BOX #2 -->
                    <div class="col" id="Marketing">
                        <div class="fbox-2 fb-2 wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">

                            <!-- Image -->
                            <div class="fbox-img gr--whitesmoke h-175">
                                <img class="img-fluid" src="assets/icmsolutions/images/icon-2.png" alt="feature-image">
                            </div>

                            <!-- Text -->
                            <div class="fbox-txt">
                                <h6 class="s-22 w-700">Marketing & Promotions</h6>
                                <p>We believe our expertise and consultative approach can make a difference to maximizing your ROI right from the start.</p>
