    <!-- HERO-1
    ============================================= -->
    <section id="hero-1" class="bg--scroll hero-section">
        <div class="container">
            <div class="row d-flex align-items-center">
                <!-- HERO TEXT -->
                <div class="col-md-6">
                    <div class="hero-1-txt color--white wow fadeInRight">
                        <!-- Title -->
                        <h2 class="s-58 w-700">Welcome to<br/>ICM Solutions</h2>
                    </div>
                </div> <!-- END HERO TEXT -->
                <!-- HERO IMAGE -->
                <div class="col-md-6">
                    <div class="hero-1-img wow fadeInLeft">
                        <img class="img-fluid" src="assets/icmsolutions/images/image-1-banner.png" alt="hero-image">
                    </div>
                </div>
            </div> <!-- End row -->
        </div> <!-- End container -->
    </section> <!-- END HERO-1 -->
    <!-- TEXT CONTENT
    ============================================= -->
    <section class="bg--white-300 py-100 ct-02 content-section division" id="About">
