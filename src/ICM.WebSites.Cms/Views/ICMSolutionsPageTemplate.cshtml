                        <p class="footer-mail-link ico-25">
                            <a href="mailto:yourdomain@mail.com">info.gr@icmsolutions.gr</a>
                        </p>

                        <!-- Social Links -->
                        <ul class="footer-socials ico-25 text-center clearfix">
                            <li>
                                <a href="https://www.linkedin.com/company/icm.com/">
                                    <span class="flaticon-linkedin-logo"></span>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.youtube.com/user/ICMCapital">
                                    <span class="flaticon-youtube"></span>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com/ICMCapital/">
                                    <span class="flaticon-instagram"></span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="flaticon-facebook"></span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="flaticon-twitter"></span>
