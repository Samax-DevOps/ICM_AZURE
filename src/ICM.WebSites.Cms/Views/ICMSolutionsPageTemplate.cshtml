                            <!-- Image -->
                            <div class="fbox-img gr--whitesmoke h-175">
                                <img class="img-fluid" src="assets/icmsolutions/images/icon-4.png" alt="feature-image">
                            </div>

                            <!-- Text -->
                            <div class="fbox-txt">
                                <h6 class="s-22 w-700">Client Services</h6>
