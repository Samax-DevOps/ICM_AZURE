        FB.init({
            appId: '192833541588035', // App ID
            status: true, // check login status
            cookie: true, // enable cookies to allow the server to access the session
            xfbml: true  // parse XFBML
        });


        FB.Event.subscribe('auth.authResponseChange', function (response)
        {
            if (response.status === 'connected')
            {
                //document.getElementById("message").innerHTML +=  "<br>Connected to Facebook";

                //SUCCESS

            } else if (response.status === 'not_authorized')
            {
                document.getElementById("message").innerHTML += "Failed to Connect";
                $('.status').html("error!");
                //FAILED
            } else
            {
                document.getElementById("message").innerHTML += "Logged Out";
                $('.status').html("error!");

                //UNKNOWN ERROR
            }
        });

    };

    function Login(formId, firstNameID, lastNameID, emailID)
    {
        $(formId+' '+'.status').html('<img src="assets/images/gif.gif" width="100" height="auto" />');
        FB.login(function (response) {
            if (response.authResponse)
            {
                getUserInfo(formId, firstNameID, lastNameID, emailID);
                $(formId+' '+'.status').html("");
            } else
            {
                $(formId+' '+'.status').html("error!");
            }
        }, {scope: 'email'});


    }

    function getUserInfo(formId, firstNameID, lastNameID, emailID) {
        FB.api('/me', {fields: 'email,first_name,last_name'}, function (response) {
            //alert(JSON.stringify(response));
            hidepopup2();
            var str = response.last_name;
            var ret = str.split(" ");
            if (response.first_name)
                $(formId+' '+firstNameID).val(response.first_name);

            if (ret[0])
                $(formId+' '+lastNameID).val(ret[0]);

            if (ret[1])
                $(formId+' '+lastNameID).val(ret[0] + ' ' + ret[1]);
            if (response.email == '') {

//$('.status').html("error!");		
            } else {

                $(formId+' '+emailID).val(response.email);
            }
        });
    }


    // Load the SDK asynchronously
    (function (d) {
        var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement('script');
        js.id = id;
        js.async = true;
        js.src = "//connect.facebook.net/en_US/all.js";
        ref.parentNode.insertBefore(js, ref);
    }(document));

    function onSuccess(googleUser) {
        $('#tc-lead-registration .status').html('<img src="assets/images/gif.gif" width="100" height="auto" />');
        hidepopup2();
        if (googleUser.getBasicProfile().getEmail() == '') {
//$('.status').html("error!");	
        } else {
            var str = googleUser.getBasicProfile().getName();
            var ret = str.split(" ");
            if (ret[0])
                $('#tc-lead-registration #first_name').val(ret[0]);

            if (ret[1])
                $('#tc-lead-registration #last_name').val(ret[1]);

            if (ret[2])
                $('#tc-lead-registration #last_name').val(ret[1] + ' ' + ret[2]);

            $('#tc-lead-registration #email').val(googleUser.getBasicProfile().getEmail());
            $('#tc-lead-registration .status').html('');
        }
    }
    function onFailure(error) {
        //console.log(error);
        $('#tc-lead-registration .status').html("error!");
    }
    
    function onSuccessBottom(googleUser) {
        $('#tc-lead-registration_bottom .status').html('<img src="assets/images/gif.gif" width="100" height="auto" />');
        hidepopup2();
        if (googleUser.getBasicProfile().getEmail() == '') {
//$('.status').html("error!");	
        } else {
            var str = googleUser.getBasicProfile().getName();
            var ret = str.split(" ");
            if (ret[0])
                $('#tc-lead-registration_bottom #first_name_bottom').val(ret[0]);

            if (ret[1])
                $('#tc-lead-registration_bottom #last_name_bottom').val(ret[1]);

            if (ret[2])
                $('#tc-lead-registration_bottom #last_name_bottom').val(ret[1] + ' ' + ret[2]);

            $('#tc-lead-registration_bottom #email_bottom').val(googleUser.getBasicProfile().getEmail());
            $('#tc-lead-registration_bottom .status').html('');
        }
    }
    function onFailureBottom(error) {
        //console.log(error);
        $('#tc-lead-registration_bottom .status').html("error!");
    }
    function renderButton() {
        gapi.signin2.render('my-signin2', {
            'scope': 'profile email',
            'width': 200,
            'height': 32,
            'longtitle': true,
            'theme': 'dark',
            'onsuccess': onSuccess,
            'onfailure': onFailure
        });
        gapi.signin2.render('my-signinBottom', {
            'scope': 'profile email',
            'width': 200,
            'height': 32,
            'longtitle': true,
            'theme': 'dark',
            'onsuccess': onSuccessBottom,
            'onfailure': onFailureBottom
        });
    }

</script>
