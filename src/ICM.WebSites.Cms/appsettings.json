{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "0d4d61a0-c6cc-4c4c-954d-a9c9f5bd8e30",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  },
  "ICM": {
    "SmtpSettings": {
        "Host": "localhost",
        "Port": 25,
        "UseSsl": false,
        "UserName": null,
        "Password": null
    },
    "EmailSettings": {
      "www.icmsolutions.gr": "info.gr@icmsolutions.gr"
    }
  }
}